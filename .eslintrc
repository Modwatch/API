{
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {
    "console": true,
    "JsDiff": true
  },
  "rules": {
    "camelcase": 1,
    "dot-notation": 1,
    "no-console": 0,
    "space-infix-ops": 1,
    "quotes": 1,
    "key-spacing": 1,
    "no-trailing-spaces": 1,
    "no-unused-vars": 0,
    "comma-spacing": 1,
    "eol-last": 1,
    "no-spaced-func": 1,
    "no-multi-spaces": 1,
    "no-mixed-spaces-and-tabs": 1,
    "handle-callback-err": 1
  }
}
