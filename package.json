{
  "name": "modwatchapi",
  "version": "3.0.2",
  "description": "Modwatch API",
  "author": {
    "name": "Aaron Ballard",
    "email": "ans.ballard@gmail.com"
  },
  "scripts": {
    "start": "pm2 start server.js",
    "restart": "pm2 restart all",
    "dev": "node -r sucrase/register/ts -r dotenv/config ./node_modules/.bin/micro-dev ./dist/server/micro.js -w ./lib/ -p 3001",
    "prettier": "prettier --write '{server.js,lib/**/*.ts}'",
    "build": "tsc && cp ./lib/server/oauth.ejs ./dist/server/"
  },
  "engines": {
    "node": ">= 12.14.0"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.2",
    "helmet": "^3.22.0",
    "jwt-simple": "^0.5.6",
    "micro": "^9.3.4",
    "micro-compress": "^1.0.0",
    "micro-cors": "^0.1.1",
    "microrouter": "^3.1.3",
    "mongo-sanitize": "^1.1.0",
    "mongodb": "^3.5.6",
    "url-pattern": "^1.0.3",
    "urlencoded-body-parser": "^3.0.0"
  },
  "devDependencies": {
    "@modwatch/core": "^0.1.0",
    "@modwatch/types": "^0.3.0",
    "@types/micro": "^7.3.3",
    "@types/microrouter": "^3.1.1",
    "@types/mongodb": "^3.5.8",
    "@types/node": "^13.13.0",
    "cross-env": "^7.0.2",
    "micro-dev": "^3.0.0",
    "pm2": "^4.2.3",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.4",
    "prompt": "^1.0.0",
    "sucrase": "^3.13.0",
    "typescript": "^3.8.3"
  },
  "private": true,
  "main": "server.js"
}
