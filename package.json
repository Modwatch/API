{
  "name": "modwatchapi",
  "version": "3.0.2",
  "description": "Modwatch API",
  "author": {
    "name": "Aaron Ballard",
    "email": "ans.ballard@gmail.com"
  },
  "scripts": {
    "start": "pm2 start server.js",
    "restart": "pm2 restart all",
    "local": "node -r esbuild-register server.js",
    "prettier": "prettier --write '{server.js,lib/**/*.ts}'",
    "build": "tsc && cp ./lib/server/oauth.ejs ./dist/server/",
    "pop": "node -r esbuild-register populate.ts",
    "db": "docker run --rm -it --name mw-db -p 27017:27017 mongo",
    "mongo": "docker exec -it mw-db mongosh"
  },
  "engines": {
    "node": ">= 12.14.0"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "dotenv": "^8.2.0",
    "ejs": "^2.7.4",
    "helmet": "^3.21.2",
    "jwt-simple": "^0.5.6",
    "micro": "^9.3.4",
    "micro-compress": "^1.0.0",
    "micro-cors": "^0.1.1",
    "microrouter": "^3.1.3",
    "mongo-sanitize": "^1.0.1",
    "mongodb": "^3.4.0",
    "url-pattern": "^1.0.3",
    "urlencoded-body-parser": "^3.0.0"
  },
  "devDependencies": {
    "@modwatch/core": "^0.1.0",
    "@modwatch/types": "^0.3.0",
    "@types/micro": "^7.3.3",
    "@types/microrouter": "^3.1.1",
    "@types/mongodb": "^3.3.13",
    "@types/node": "^10.17.11",
    "cross-env": "^7.0.3",
    "esbuild-register": "^3.3.3",
    "micro-dev": "^3.0.0",
    "minimist": "^1.2.6",
    "node-fetch": "^3.2.10",
    "pm2": "^3.5.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "typescript": "^3.7.3"
  },
  "private": true,
  "main": "server.js"
}
